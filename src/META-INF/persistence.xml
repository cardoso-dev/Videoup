    <class>com.videoup.entities.VideoupCustomers</class>
    <class>com.videoup.entities.VideoupCatgs</class>
    <class>com.videoup.entities.VideoupFormts</class>
    <class>com.videoup.entities.VideoupTaxes</class>
    <class>com.videoup.entities.VideoupGames</class>
    <class>com.videoup.entities.VideoupCstmrnotes</class>
    <class>com.videoup.entities.VideoupVentas</class>
    <class>com.videoup.entities.VideoupItemsvnt</class>
    <class>com.videoup.entities.VideoupConf</class>
    <class>com.videoup.entities.VideoupBcodes</class>
    <class>com.videoup.entities.VideoupAutrz</class>
