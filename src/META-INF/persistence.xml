  <persistence-unit name="VideoupPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>com.videoup.entities.VideoupItemsrnt</class>
    <class>com.videoup.entities.VideoupCtofrentas</class>
